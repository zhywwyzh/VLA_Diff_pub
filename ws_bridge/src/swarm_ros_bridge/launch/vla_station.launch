<?xml version="1.0"?>
<launch>
    <group ns="bridge">
        <node pkg="swarm_ros_bridge" type="bridge_new" name="swarm_bridge_node_station" output="screen" >
            <param name="hostname" type="string" value="groundStation0"/>
            <rosparam command="load" file="$(find swarm_ros_bridge)/config/default_vla.yaml" />
            <rosparam command="load" file="$(find swarm_ros_bridge)/config/ip_real_vla.yaml" />
        </node>
    </group>
</launch>
